<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>boosted.sass</id>
    <version>4.1.3</version>
    <title>Boosted Sass</title>
    <authors>The Boosted Authors, Orange, The Bootstrap Authors, Twitter Inc.</authors>
    <owners>Orange</owners>
    <description>Fork of the most popular front-end framework for developing responsive, mobile first projects on the web.</description>
    <releaseNotes>https://github.com/Orange-OpenSource/Orange-Boosted-Bootstrap/releases</releaseNotes>
    <summary>Boosted framework in SCSS. Includes fonts and JavaScript</summary>
    <language>en-us</language>
    <projectUrl>http://boosted.orange.com</projectUrl>
    <iconUrl>http://boosted.orange.com/apple-touch-icon.png</iconUrl>
    <licenseUrl>https://github.com/Orange-OpenSource/Orange-Boosted-Bootstrap/blob/v4-dev/LICENSE</licenseUrl>
    <copyright>Copyright 2017-2018</copyright>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <dependency id="jQuery" version="[3.0.0,4)" />
      <dependency id="popper.js" version="[1.14.0,2)" />
      <dependency id="tablesorter.js" version="[2.25.0,3)" />
    </dependencies>
    <tags>css sass mobile-first responsive front-end framework web</tags>
  </metadata>
  <files>
    <file src="scss\**\*.scss" target="content\Content\boosted" />
    <file src="dist\js\boosted*.js" target="content\Scripts" />
    <file src="dist\js\boosted*.js.map" target="content\Scripts" />

    <file src="scss\**\*.scss" target="contentFiles\Content\boosted" />
    <file src="dist\js\boosted*.js" target="contentFiles\Scripts" />
    <file src="dist\js\boosted*.js.map" target="contentFiles\Scripts" />
  </files>
</package>
